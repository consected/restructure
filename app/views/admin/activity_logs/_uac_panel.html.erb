<div id="user-access-controls-embedded">
  <h4>User Access Controls for Activity Table</h4>
  <% if object_instance.persisted? %>

  <div class="admin-result-index" id="embedded-al-table-uac-embedded">
    <%= link_to "Load", admin_user_access_controls_path(
                          filter: { resource_name: "#{object_instance.full_item_type_name.pluralize}", resource_type: "table", app_type_id: '' }, 
                          view_as: 'simple-embedded', readonly: true), 
                          'data-remote': true,
                          'data-target-force': true,
                          'data-target': '#embedded-al-table-uac-embedded'
                          %>
  </div>
  <p>
    <%= link_to "View all table access controls", admin_user_access_controls_path(filter: { resource_name: "#{object_instance.full_item_type_name.pluralize}", resource_type: "table", app_type_id: '' }), target: '_blank' %>
  </p>  

  <h4>User Access Controls for Process Activities</h4>
  <div class="admin-result-index" id="embedded-al-uac-embedded">
    <%= link_to "Load", admin_user_access_controls_path(
                          filter: { resource_name: "#{object_instance.full_item_type_name}__%", app_type_id: '' }, 
                          view_as: 'simple-embedded', readonly: true), 
                          'data-remote': true,
                          'data-target-force': true,
                          'data-target': '#embedded-al-uac-embedded'
                          %>
  </div>
  <p>
    <%= link_to "View all process activity access controls", admin_user_access_controls_path(filter: { resource_name: "#{object_instance.full_item_type_name}__%", app_type_id: ''  }), target: '_blank' %>
  </p>
  <% end %>
</div>