<div class="data-results">
  <div data-result="admin-edit-form-" class="admin-edit-form">
<%= form_for(object_instance, class: "inline-form", remote: true) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :app_type_id %>
    <% select_item_type = {}
       select_item_type[:selected] = params[:filter].first.last if params[:filter] && params[:filter].first.first == 'app_type_id'
    %>
    <%= f.select :app_type_id, app_type_options, select_item_type, extra_field_attributes[:app_type_id]||{} %>
  </div>
  <div class="form-group">
    <%= f.label :name %>
    <% select_item_type = {}
       select_item_type[:selected] = params[:filter].first.last if params[:filter] && params[:filter].first.first == 'name'    %>

    <%= f.select :name, app_configuration_options, select_item_type %>
  </div>
  <div class="form-group">
    <%= f.label :value %>
    <%= f.text_field :value %>
  </div>
  <div class="form-group">
    <%= f.label 'role name override' %>
    <%
      select_item_type = {}
      select_item_type[:selected] = params[:filter].first.last if params[:filter] && params[:filter].first.first.to_s == 'role_name'
      select_item_type[:include_blank] = true

      options = role_name_options
    %>
    <%= f.select :role_name, options, select_item_type %>
  </div>
  <div class="form-group">
    <%= f.label 'user override' %>
    <%= f.select :user_id, active_user_options, include_blank: true %>
  </div>
  <div class="form-group">
    <%= f.check_box :disabled %>
    <%= f.label :disable %>
  </div>


  <div class="form-group">

    <%= hidden_filter_fields %>
    <%= f.submit class: "btn btn-primary" %> <%= admin_edit_cancel %>
  </div>
<% end %>
  </div>
</div>
