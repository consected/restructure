<div class="data-results">
  <div data-result="admin-edit-form-" class="admin-edit-form">
<%= form_for([:admin, object_instance], class: "inline-form", remote: true) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :app_type %>
    <%= f.select :name, AppType.active.map{|a| [a.label, a.name]}%>
  </div>
  <div class="form-group">
    <%= f.label 'user override' %>
    <%= f.select :user_id, options_from_collection_for_select(User.active, 'id', 'email', object_instance.user_id), include_blank: true %>
  </div>
  <div class="form-group">
    <%= f.label :access %>
    <%= f.text_field :access%>
  </div>
  <div class="form-group">
    <%= f.label :resource_type %>
    <%= f.select :resource_type, UserAccessControl.resource_types %>
  </div>
  <div class="form-group">
    <%= f.label :resource_name %>
    <%= f.select :resource_name, UserAccessControl.resource_names %>
  </div>
  <div class="form-group">
    <%= f.label :option %>
    <%= f.text_field :option %>
  </div>
  <div class="form-group">
    <%= f.check_box :disabled %>
    <%= f.label :disable %>
  </div>


  <div class="form-group">
    <%= f.submit class: "btn btn-primary" %> <%= admin_edit_cancel %>
  </div>
<% end %>
  </div>
</div>
