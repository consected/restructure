<div class="admin-edit-form alert" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>
      <p>Email: <%= @user.email %></p>
      <% if @user.new_password %>
      <p>New password: <%= @user.new_password %> </p>
      <p>New token: <%= @user.new_token %> </p>
      <% end %>
      <p>Disabled:<%=!!@user.disabled%></p>
      <p>
       <%
           html = render partial: 'credential_doc'
        %>
        <a href="data:text/html;base64,<%=Base64.encode64(html)%>" download="qrcode-<%=@user.email.split('@').first%>" title="click to download">Download Credentials Document</a>
      </p>

    </div>
