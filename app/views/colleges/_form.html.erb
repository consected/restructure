<div class="data-results">
  <div data-result="admin-edit-form-" class="admin-edit-form">    
<%= form_for(object_instance, class: "inline-form", remote: true) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :name, 'data-add-icon' => 'info-sign', title: "#{object_instance.id ? 'Name for existing records can not be changed.' : 'Set name'}" %>
    <%= f.text_field :name, disabled: !!object_instance.id%>
  </div>
  <div class="form-group">
    <%= f.label :synonym_for_id %>
    <%= f.select :synonym_for_id, College.selector_array_pair.sort_by {|n| n.first}, include_blank: '(none)' %>
  </div>
  <div class="form-group">
    <%= f.label :disable %>
    <%= f.check_box :disabled %>
  </div>
  <div class="form-group">
    <%= f.submit class: "btn btn-primary" %> <%= admin_edit_cancel %>
  </div>
<% end %>
  </div>
</div>