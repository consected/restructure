<p id="notice"><%= notice %></p>

<h1>Listing Protocols</h1>

<p>
    <%=link_to "all events", protocol_events_path(all: true)%>
</p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Admin</th>
      <th>Disabled</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protocols.each do |protocol| %>
      <tr>
        <td><%= protocol.name %></td>
        <td><%= protocol.admin_name %></td>
        <td><%= protocol.disabled %></td>
        <td><%= link_to "edit sub processes", protocol_sub_processes_path(protocol) %> </td>
        
        <td><%= link_to 'Edit', url_for(id: protocol, action: :edit) %></td>        
        
      </tr>
    <% end %>
  </tbody>
</table>

<%=  render partial: 'admin_handler/index_actions'  %>