<%
  
  @panels.each do |resource|
    resource_name = resource[:resource_name]
    resource_type = resource[:type].to_s.pluralize
    template_resource = resource[:hyphenated_name].singularize
%>
  <div id="user-profile-<%= template_resource %>-{{user.id}}" 
       class="<%= resource_type %>-user-profile-block <%= template_resource %>-block collapse" 
       role="tabpanel" 
       aria-labelledby="tab-user-profile-<%= template_resource %>-{{user.id}}" 
  >    
    <div id="<%= template_resource %>--{{<%= resource_name %>.id}}" 
         class="<%= resource_type %>-user-profile-inner-block user-profile-resource-block"
         form-res-template="<%= template_resource %>-result-template"
         data-subscription="<%= template_resource %>-edit-form--{{<%=resource_name%>.id}}"
    >
    </div>
  </div>
<%
  
  end
%>
