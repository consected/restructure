<!DOCTYPE html>
<html>
<head>
  <title>FPHS</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
    <%= render partial: 'layouts/navbar' %>
    <div class="flash">
      <% flash.each do |key, value| 
          f_class = 'alert-info'
          f_class = "alert-#{key.to_s}" unless key == 'notice'
      %>
      <p class="alert <%= f_class %>">
        <%= value %>
      </p>
      <% end %>
    </div>

<%= yield %>

</body>
</html>
