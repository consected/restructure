<%
if @report.view_css.classes
  outer = "##{@outer_block_id}-results-block"
  res = @report.view_css.classes.map {|c, v|
    styles = v.map{|s,v| "#{s.to_s.hyphenate}: #{v};"}
    "#{outer} .#{c} \{  #{styles.join(' ')}\}"
  }
  res = res.join("\n")
%>
<style>
<%= res.html_safe %>
</style>
<% end %>
