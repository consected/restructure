<%
  dm = DynamicModel.active_model_configurations.where(category: category)
  dm = dm.select {|m| m.resource_name.to_sym.in? @true_views}
%>
<div class="row panel-body panel-with-sublists dynamic-models-panel">
  <%= render partial: 'masters/dynamic_model_blocks', locals: { category_models: dm, viewable: view?, category: category, force_load: true, panel: panel } %>
</div>
