<%
  resources = panel.contains.resources
  init_visible = panel.view_options.initial_show || nil if panel.view_options
  resources.each do |resource|
    if view?[resource.to_sym]
      rs = resource.split('__', 2)
      if rs.length == 2
        resource_type = rs.first.hyphenate.pluralize
      else
        resource_type = 'details'
      end
%>
    <div id="<%= resource.hyphenate %>-{{id}}" class="<%= resource_type %>-generic-block <%= resource.hyphenate %>-block collapse" data-sub-for-root="master_id" data-sub-id="{{id}}" data-sub-item="<%= resource %>" data-template="<%= resource.hyphenate %>-main-result-template"  >
        <div id="<%= resource.hyphenate %>-inner-{{id}}" class="<%= resource_type %>-inner-block">
        </div>
    </div>
<%
    end
  end
%>
