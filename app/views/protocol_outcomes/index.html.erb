<p id="notice"><%= notice %></p>

<h1>Listing Protocol Outcomes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Protocol</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protocol_outcomes.each do |protocol_outcome| %>
      <tr>
        <td><%= protocol_outcome.name %></td>
        <td><%= protocol_outcome.protocol.name %></td>
        <td><%= protocol_outcome.admin %></td>
        <td><%= link_to 'Show', protocol_outcome %></td>
        <td><%= link_to 'Edit', edit_protocol_outcome_path(protocol_outcome) %></td>
        <td><%= link_to 'Destroy', protocol_outcome, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protocol outcome', new_protocol_outcome_path %>
