<div class="data-results">
  <div data-result="admin-edit-form-"  class="admin-edit-form">    
<%= form_for(object_instance, class: "inline-form", remote: true) do |f| %>
  <%= render partial: 'admin_handler/form_errors' %>
  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name %>
  </div>
  <div class="form-group">
    <%= f.label :value, 'data-add-icon' => 'info-sign', title: "#{object_instance.id ? 'Value for existing records can not be changed.' : 'Set internal value'}" %>
    <%= f.number_field :value, disabled: !!object_instance.id %>
  </div>
  <div class="form-group">
    <%= f.check_box :disabled %>
    <%= f.label :disable %>
    
  </div>
  <div class="form-group">
    <%= f.submit class: "btn btn-primary" %> <%= admin_edit_cancel %>
  </div>

  <% end %>

  </div>
</div>