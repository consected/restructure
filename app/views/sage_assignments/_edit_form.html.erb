<div class="data-results">
  <div data-result="<%= edit_form_id %>">
    <div class="">
      <%=form_for([@master, object_instance], edit_form_hash ) do |f|%>
      <ul class="list-group">
        <li class="list-group-item">
          <%= inline_cancel_button %>
          <h4 class="list-group-item-heading">Assign Sage ID</h4>
        </li>
        <li class="list-group-item player-info-first_name hidden">                    
            <%= f.hidden_field :sage_id %>
        </li>  
        
        <li class="list-group-item">
          <%= f.submit class: "btn btn-primary" %>  
        </li>
      </ul>
      <% end %>
    </div>
  </div>
</div>